  "scripts": {
    "start": "node dist/src/server.js",
    "dev": "concurrently \"tsx watch src/server.ts\"",
    "build": "tsc && npm run build:docs",
    "build:docs": "npx @11ty/eleventy",
    "serve:docs": "npx @11ty/eleventy --serve --port=8080",
    "prestart": "npm run build:docs",
    "clean": "rm -rf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json}\"",
    "drizzle:push": "npx drizzle-kit push",
    "drizzle:generate": "npx drizzle-kit generate",
    "drizzle:migrate": "npx drizzle-kit migrate",
    "drizzle:studio": "npx drizzle-kit studio",
    "seed": "tsx scripts/seed.ts",
    "workers:all": "ts-node ./src/workers.ts",
    "workers:auth": "ts-node ./src/auth/workers.ts",
    "chroma:import-laws": "tsx scripts/import-laws-to-chroma.ts",
    "chroma:search-laws": "tsx scripts/retrieve-laws-from-chroma.ts",
    "test:embeddings": "ts-node src/lib/llm/ollama/ollama.embeddings.ts",
    "ollama:list": "ts-node scripts/manage-ollama-models.ts list",
    "ollama:pull": "ts-node scripts/manage-ollama-models.ts pull nomic-embed-text",
    "postinstall": "npm run build && npm run ollama:pull",
    "postbuild": "npm run ollama:pull"
  },
